{
  "name": "grapple",
  "version": "1.0.0",
  "description": "A Postman-like UI client to interact with gRPC services",
  "main": "main.js",
  "scripts": {
    "build": "npm run compile && npm run dist",
    "ci:test": "npm run test -- --coverage",
    "ci:lint": "npm run lint",
