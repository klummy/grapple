{
  "name": "grapple",
  "version": "1.0.0",
  "description": "A Postman-like UI client to interact with gRPC services",
  "main": "main.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"yarn:dev:*\"",
    "dev:serve": "parcel index.html",
    "dev:start": "NODE_ENV=development electron .",
    "start": "electron dist",
    "test": "jest",
    "ts:watch": "tsc -w"
  },
  "keywords": [],
  "author": "Yekeen Ajeigbe <aayekeen@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.4",
    "concurrently": "^4.1.0",
    "electron": "^4.0.0-beta.9",
    "electron-builder": "^20.38.3",
    "electron-reload": "^1.4.0",
    "parcel": "^1.10.3",
    "prettier": "^1.15.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "build": {
    "appId": "com.nerdge.grapple311",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "files": [
      "main.js",
      "dist/*"
    ]
  },
  "dependencies": {
    "components": "^0.1.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.3"
  }
}
