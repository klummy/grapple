{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "max-line-length": [true, 160],
    "member-access": false,
    "array-type": [false],
    "ordered-imports": [true],
    "object-literal-sort-keys": true,
    "no-var-requires": false,
    "interface-name": [true, "always-prefix"]
  },
  "rulesDirectory": []
}